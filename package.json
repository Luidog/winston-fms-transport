{
  "name": "winston-fms-transport",
  "version": "1.0.1",
  "description":
    "A transport to support logging via winston to FileMaker Server",
  "main": "index.js",
  "scripts": {
    "test": "nyc _mocha --recursive ./tests --timeout=30000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "generate-readme":
      "node_modules/.bin/readme package.json --tests --travis > README.md",
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.js --verbose",
    "example": "node example.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Luidog@github.com/Luidog/winston-fms-transport.git"
  },
  "keywords": ["winston", "FileMaker", "fms-api-client", "marpat", "logs"],
  "author": "Lui de la Parra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Luidog/winston-fms-transport/issues"
  },
  "homepage": "https://github.com/Luidog/winston-fms-transport#readme",
  "dependencies": {
    "fms-api-client": "^1.7.1",
    "marpat": "^1.19.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "dotenv": "^5.0.1",
    "eslint": "^4.11.0",
    "eslint-config-google": "^0.9.1",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jsdocs": "^1.0.0",
    "minami": "^1.2.3",
    "mocha": "^5.0.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.8.0",
    "prettier": "^1.8.2",
    "varium": "^1.0.1"
  }
}
